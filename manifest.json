{
    "manifest_version": 3,
    "name": "Location Extension",
    "version": "1.0.0",
    "description": "Displays user's country and city",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "128": "images/icon128.png"
    }
  }
  